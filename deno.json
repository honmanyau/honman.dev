{
	"lock": false,
	"tasks": {
		"check": "deno fmt --check && deno lint && deno check **/*.ts && deno check **/*.tsx",
		"cli": "echo \"import '\\$fresh/src/dev/cli.ts'\" | deno run --unstable -A -",
		"manifest": "deno task cli manifest $(pwd)",
		"start": "GITHUB_CLIENT_ID='Ov23liZf8cmT8uyyShdK' GITHUB_CLIENT_SECRET='38e3ec8df5a11c23eaeb3aa1780ff8389899e108' deno run -A --watch=static/,routes/ --unstable-kv dev.ts",
		"build": "GITHUB_CLIENT_ID='Ov23liZf8cmT8uyyShdK' GITHUB_CLIENT_SECRET='38e3ec8df5a11c23eaeb3aa1780ff8389899e108' deno run -A --unstable-kv dev.ts build",
		"preview": "deno run -A main.ts",
		"update": "deno run -A -r https://fresh.deno.dev/update ."
	},
	"lint": {
		"rules": {
			"tags": [
				"fresh",
				"recommended"
			]
		}
	},
	"fmt": {
		"useTabs": true,
		"lineWidth": 80,
		"indentWidth": 4
	},
	"exclude": [
		"**/_fresh/*"
	],
	"imports": {
		"$fresh/": "https://deno.land/x/fresh@1.7.2/",
		"$std/": "https://deno.land/std@0.216.0/",
		"@/": "./",
		"@deno/kv-oauth": "jsr:@deno/kv-oauth@^0.11.0",
		"@feed/feed": "jsr:@feed/feed@^1.1.0",
		"@preact/signals": "https://esm.sh/*@preact/signals@1.2.2",
		"@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.1",
		"@std/expect": "jsr:@std/expect@^1.0.5",
		"@std/testing": "jsr:@std/testing@^1.0.3",
		"@std/text": "jsr:@std/text@^1.0.8",
		"dompurify": "npm:dompurify@^3.1.7",
		"highlight.js": "npm:highlight.js@^11.10.0",
		"jsdom": "npm:jsdom@^25.0.1",
		"marked": "npm:marked@^14.1.3",
		"marked-highlight": "npm:marked-highlight@^2.2.0",
		"preact": "https://esm.sh/preact@10.22.0",
		"preact/": "https://esm.sh/preact@10.22.0/",
		"zod": "npm:zod@^3.23.8"
	},
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "preact"
	},
	"nodeModulesDir": "auto"
}
